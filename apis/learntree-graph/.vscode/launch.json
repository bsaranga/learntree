{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Learn Tree Graph API [Launch]",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/learntree_graph.api/bin/Debug/net6.0/leantree_graph.dll",
            "args": [],
            "cwd": "${workspaceFolder}/learntree_graph.api",
            "stopAtEntry": false,
            "justMyCode": false,
            "sourceLinkOptions": {
                "*": {
                    "enabled": true
                }
            },
            "symbolOptions": {
                "searchMicrosoftSymbolServer": true,
                "searchNuGetOrgSymbolServer": true
            },
            "suppressJITOptimizations": true,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "COMPlus_ReadyToRun": "0"
            },
        },
        {
            "name": "Attach",
            "type": "coreclr",
            "request": "attach",
            "justMyCode": false,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": true,
                "searchNuGetOrgSymbolServer": true
            },
            "suppressJITOptimizations": true,
            "env": {
                "COMPlus_ReadyToRun": "0"
            }
        }
    ]
}